{
  "manifest_version": 2,
  "name": "Cookie Sniffer",
  "version": "0.1",
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "<all_urls>" ],
  "background": {
    "scripts": ["background.js", "jquery-2.1.3.min.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js", "sniffer.js"]
    }
  ]
}
